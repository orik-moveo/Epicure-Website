{
  "name": "@my-app/backend-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "backend-e2e",
    "implicitDependencies": [
      "backend"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "packages/backend-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "backend:build",
          "backend:serve"
        ]
      }
    }
  }
}
